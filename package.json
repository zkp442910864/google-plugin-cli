{
    "name": "plugin1",
    "version": "0.0.0",
    "scripts": {
        "dev-raw": "vite",
        "test": "vite build --mode none",
        "serve:web": "vite preview",
        "build:web": "vite build --mode production",
        "build:crx3": "crx3 -p ./build/key.pem -o ./build/dist3.crx -z ./build/dist3.zip ./dist",
        "build:crx2": "crx pack ./dist --crx-version 2 -o ./build/dist2.crx -p ./build/key.pem",
        "build:zip2": "crx pack ./dist --zip-output ./build/dist2.zip -p ./build/key.pem ",
        "build:ff": "web-ext build -c=firefox.config.js --filename distff.zip",
        "build:all": "npm run build:crx2 && npm run build:zip2 && npm run build:crx3 && npm run build:ff",
        "build": "md build 2>NUL & npm-run-all build:web build:all",
        "sign:ff": "web-ext sign -c=firefox.config.js",
        "dev": "nodemon --watch vite.config.ts --watch vite.config.tpl.ts --watch src/**/*.* --exec \"vite build --mode development\"",
        "dev:f": "web-ext run --source-dir ./dist --target=firefox-desktop --firefox=\"C:\\Program Files\\Mozilla Firefox\\firefox.exe\"",
        "dev:c": "web-ext run --source-dir ./dist --target=chromium",
        "dev:chrome": "npm run build:web && npm-run-all --parallel dev dev:c",
        "dev:firefox": "npm run build:web && npm-run-all --parallel dev dev:f"
    },
    "dependencies": {
        "ant-design-vue": "^2.2.6",
        "axios": "^0.21.3",
        "jquery": "^3.6.0",
        "moment": "^2.29.1",
        "qs": "^6.10.1",
        "vue": "^3.2.6",
        "webextension-polyfill": "^0.9.0"
    },
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-buble": "^0.21.3",
        "@rollup/plugin-commonjs": "^20.0.0",
        "@rollup/plugin-data-uri": "^0.1.1",
        "@rollup/plugin-inject": "^4.0.2",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.5",
        "@rollup/plugin-typescript": "^8.2.5",
        "@types/chrome": "^0.0.191",
        "@types/jquery": "^3.5.6",
        "@types/node": "^16.10.2",
        "@types/qs": "^6.9.7",
        "@types/webextension-polyfill": "^0.8.3",
        "@typescript-eslint/eslint-plugin": "^4.32.0",
        "@typescript-eslint/parser": "^4.32.0",
        "@vitejs/plugin-legacy": "^1.6.0",
        "@vitejs/plugin-vue": "^2.3.3",
        "@vitejs/plugin-vue-jsx": "^1.3.10",
        "@vue/compiler-sfc": "^3.2.19",
        "code-splitting": "^1.0.0",
        "cross-env": "^7.0.3",
        "crx": "^5.0.1",
        "crx3": "^1.1.3",
        "eslint": "^7.32.0",
        "eslint-plugin-vue": "^7.18.0",
        "fs": "^0.0.1-security",
        "less": "^4.1.1",
        "nodemon": "^2.0.13",
        "npm-run-all": "^4.1.5",
        "path": "^0.12.7",
        "rollup": "2.53.1",
        "rollup-plugin-clear": "^2.0.7",
        "sign-addon": "^4.1.0",
        "typescript": "^4.4.3",
        "vite": "^2.9.9",
        "vite-plugin-components": "^0.13.3",
        "vite-plugin-imp": "^2.0.9",
        "vite-plugin-importer": "^0.2.5",
        "vue-tsc": "^0.3.0",
        "web-ext": "^6.8.0"
    }
}
